	"item": [
		{
			"name": "Login",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const response = pm.response.json()\r",
							"pm.collectionVariables.set(\"token\", response[\"access_token\"])"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"pm.sendRequest({\r",
							"    url: pm.collectionVariables.get(\"base_url\")+\"/login\",\r",
							"    method: 'GET'\r",
							"}, function (err, res) {\r",
							"    let csrfTokenMatch = res.text().match(/name=\"_csrf\" type=\"hidden\" value=\"([^\"]+)\"/);\r",
							"    if (csrfTokenMatch && csrfTokenMatch[1]) {\r",
							"        pm.environment.set('csrf_token', csrfTokenMatch[1]);\r",
							"    } else {\r",
							"        console.error('CSRF token not found');\r",
							"    }\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"auth": {
					"type": "noauth"
				},
				"method": "POST",
				"header": [],
				"body": {
					"mode": "formdata",
					"formdata": [
						{
							"key": "username",
							"value": "admin@kitchensink.com",
							"type": "text"
						},
						{
							"key": "password",
							"value": "password1234",
							"type": "text"
						},
						{
							"key": "_csrf",
							"value": "{{csrf_token}}",
							"type": "text"
						}
					]
				},
				"url": {
					"raw": "{{base_url}}/login",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"login"
					]
				}
			},
			"response": []
		},
